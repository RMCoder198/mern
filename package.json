{
  "name": "mern",
  "version": "1.0.0",
  "description": "mern + elasticsearch project",
  "main": "app.js",
  "scripts": {
    "start-old": "webpack-dev-server --hot",
    "clean": "rm -rf build public/bundle.js",
    "build": "babel server --out-dir build --presets=es2015 && webpack",
    "start": "cross-env NODE_ENV=production node ./build/app.js",
    "development": "cross-env NODE_ENV=development nodemon --exec babel-node --presets=es2015 ./server/app.js --watch server"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "mongodb",
    "express",
    "nodejs",
    "reactjs",
    "elasticsearch"
  ],
